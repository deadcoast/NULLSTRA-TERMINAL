{
  "name": "midaterminal",
  "version": "1.0.0",
  "description": "Advanced terminal application with comprehensive theme management",
  "main": "server/index.ts",
  "scripts": {
    "start": "next start",
    "dev": "concurrently \"next dev\" \"nodemon --watch server --watch types --ext ts,js,json --exec ts-node --project tsconfig.server.json server/index.ts\"",
    "build": "next build",
    "build:server": "tsc --project tsconfig.server.json",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "test": "jest",
    "server": "ts-node --project tsconfig.server.json server/index.ts"
  },
  "keywords": [
    "terminal",
    "themes",
    "debugging",
    "extensions"
  ],
  "author": "MidaTerminal Team",
  "license": "MIT",
  "dependencies": {
    "@types/styled-components": "^5.1.34",
    "classnames": "^2.5.1",
    "cors": "^2.8.5",
    "critters": "^0.0.23",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "jest-axe": "^10.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "next": "^15.3.1",
    "react-window": "^1.8.11",
    "redis": "^4.7.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "styled-components": "^6.1.17",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.3.1",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^15.0.0",
    "@types/classnames": "^2.3.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^5.0.6",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.14.1",
    "@types/react-window": "^1.8.8",
    "@types/redis": "^4.0.10",
    "@types/testing-library__jest-dom": "^5.14.9",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.10.3",
    "concurrently": "^9.1.2",
    "eslint": "^8.53.0",
    "eslint-plugin-import": "^2.29.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.0.1",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
